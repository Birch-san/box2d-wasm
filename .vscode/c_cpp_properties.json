{
  "env": {
    "workspaceIncludes": ["${workspaceFolder}/box2d/include"]
  },
  "configurations": [
    {
      "name": "Mac with Brew-installed Emscripten",
      "includePath": ["${default}", "${workspaceIncludes}"],
      "macFrameworkPath": [
        "/System/Library/Frameworks",
        "/Library/Frameworks"
      ],
      "forcedInclude": ["${workspaceFolder}/ide-include.h"],
      "compilerPath": "/usr/local/bin/em++",
      "cStandard": "c11",
      "cppStandard": "c++20"
    }
  ],
  "version": 4
}