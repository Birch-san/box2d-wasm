#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/birch/git/box2d-wasm/node_modules/.pnpm/eslint@7.11.0/node_modules/eslint/bin/eslint.js/node_modules:/Users/birch/git/box2d-wasm/node_modules/.pnpm/eslint@7.11.0/node_modules/eslint/bin/node_modules:/Users/birch/git/box2d-wasm/node_modules/.pnpm/eslint@7.11.0/node_modules/eslint/node_modules:/Users/birch/git/box2d-wasm/node_modules/.pnpm/eslint@7.11.0/node_modules:/Users/birch/git/box2d-wasm/node_modules/.pnpm/node_modules:/Users/birch/git/box2d-wasm/node_modules:/Users/birch/git/node_modules:/Users/birch/node_modules:/Users/node_modules:/node_modules:/Users/birch/git/box2d-wasm/integration-test-backend/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/eslint@7.11.0/node_modules/eslint/bin/eslint.js" "$@"
else 
  exec node  "$basedir/../../../node_modules/.pnpm/eslint@7.11.0/node_modules/eslint/bin/eslint.js" "$@"
fi
