#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/birch/git/box2d-wasm/node_modules/.pnpm/ts-node@9.0.0_typescript@4.0.3/node_modules/ts-node/dist/bin.js/node_modules:/Users/birch/git/box2d-wasm/node_modules/.pnpm/ts-node@9.0.0_typescript@4.0.3/node_modules/ts-node/dist/node_modules:/Users/birch/git/box2d-wasm/node_modules/.pnpm/ts-node@9.0.0_typescript@4.0.3/node_modules/ts-node/node_modules:/Users/birch/git/box2d-wasm/node_modules/.pnpm/ts-node@9.0.0_typescript@4.0.3/node_modules:/Users/birch/git/box2d-wasm/node_modules/.pnpm/node_modules:/Users/birch/git/box2d-wasm/node_modules:/Users/birch/git/node_modules:/Users/birch/node_modules:/Users/node_modules:/node_modules:/Users/birch/git/box2d-wasm/integration-test-backend/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/ts-node@9.0.0_typescript@4.0.3/node_modules/ts-node/dist/bin.js" "$@"
else 
  exec node  "$basedir/../../../node_modules/.pnpm/ts-node@9.0.0_typescript@4.0.3/node_modules/ts-node/dist/bin.js" "$@"
fi
